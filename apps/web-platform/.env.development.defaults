# .env.development.defaults
# Non-secret configuration for local development — safe to commit.
# Secrets (DATABASE_URL, JWT_SECRET, COOKIE_SECRET) are in AWS SSM.
# Run: ./tools/scripts/pull-secrets.sh --app web --env dev

# Server
NODE_ENV=development
PORT=3001
HOST=0.0.0.0
API_PREFIX=/api
API_VERSION=v1

# Database — secret, fetched from SSM
# DATABASE_URL=<from SSM>

# Redis
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# Authentication — secrets fetched from SSM
# JWT_SECRET=<from SSM>
# COOKIE_SECRET=<from SSM>
JWT_EXPIRES_IN=1h
JWT_REFRESH_EXPIRES_IN=7d

# DART API
DART_API_BASE_URL=https://opendart.fss.or.kr/api
DART_API_RATE_LIMIT=1000

# Document Extractor
DOCUMENT_EXTRACTOR_API_URL=http://localhost:8000

# AWS / S3
AWS_REGION=ap-east-1
S3_REGION=ap-east-1
S3_BUCKET_NAME=pdfs-128638789653

# URLs
FRONTEND_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3001

# Logging
LOG_LEVEL=debug

# Queue / Worker Configuration
BULL_DEFAULT_REMOVE_ON_COMPLETE=10
BULL_DEFAULT_REMOVE_ON_FAIL=10
FILING_SYNC_CONCURRENCY=2
DOCUMENT_PROCESSING_CONCURRENCY=5
PDF_PROCESSING_CONCURRENCY=2
TABLE_EXTRACTION_CONCURRENCY=1

# Workers
DISABLE_SYNC_WORKERS=false
